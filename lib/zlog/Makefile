#Makefile generated by zmake, based on ZMAKE

.PHONY: all
all: zlog libzlog.a 
lib_header=-I ../../..//app/lib/configure/include \
-I ../../..//app/lib/exception/include \

lib_library=../../..//app/lib/configure/libconfigure.a \
../../..//app/lib/exception/libexception.a \
-lpthread

zlog.o: ./src/zlog.cpp ./include/zlog.h 
	g++ -O2 -c -o output/zlog.o ./src/zlog.cpp -I ./include  $(lib_header)
test.o: ./test/test.cpp ./include/zlog.h 
	g++ -O2 -c -o output/test.o ./test/test.cpp -I ./include  $(lib_header)
zlog:test.o zlog.o 
	g++ -o output/zlog output/test.o output/zlog.o $(lib_library)
	@cp output/zlog .
libzlog.a:zlog.o 
	ar cqs -o output/libzlog.a output/zlog.o 
	@cp output/libzlog.a .
clean:
	rm -rf output/zlog.o output/test.o output/zlog zlog output/libzlog.a libzlog.a 
